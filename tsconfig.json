{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "noEmit": true,
    "jsx": "preserve",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "jsxImportSource": "preact",
    "paths": {
      "*": ["./assets/js/typings/*"]
    }
  },
  "include": ["assets/js/*"]
}
