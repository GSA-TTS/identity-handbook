{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "noEmit": true,
    "jsx": "preserve",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "jsxImportSource": "preact",
    "baseUrl": ".",
    "paths": {
      "*": ["assets/js/typings/*"]
    },
    "lib": ["ES2020"]
  },
  "include": ["assets/js/*"]
}
