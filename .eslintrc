{
  "extends": "plugin:@18f/eslint-plugin-identity/recommended",
  "plugins": ["@18f/eslint-plugin-identity"],
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "pragma": "h",
      "fragment": "Fragment",
      "version": "16.2"
    }
  },
  "rules": {
    "react/jsx-uses-react": "error"
  }
}
